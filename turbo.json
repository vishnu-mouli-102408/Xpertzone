{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "**/.env.*local",
    "**/.env",
    ".env",
    ".env.local",
    "tsconfig.json"
  ],
  "globalEnv": [
    "NEXT_PUBLIC_APP_URL",
    "REDIS_URL",
    "DATABASE_URL",
    "PORT",
    "CACHE_EXPIRE",
    "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME",
    "NEXT_PUBLIC_CLOUDINARY_API_KEY",
    "NEXT_PUBLIC_CLOUDINARY_API_SECRET",
    "NEXT_PUBLIC_PRO_PLAN_PRICE_ID",
    "NEXT_PUBLIC_ENTERPRISE_PLAN_PRICE_ID",
    "NEXT_PUBLIC_STRIPE_SECRET_KEY",
    "CLERK_SECRET_KEY",
    "SIGNING_SECRET",
    "TURBO_TOKEN",
    "STRIPE_WEBHOOK_SECRET",
    "RESEND_API_KEY",
    "TURBO_TEAM"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "dependsOn": ["dev", "^db:generate"],
      "cache": false,
      "persistent": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "lint": {
      "outputs": [".cache/.eslintcache"],
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "outputs": [".cache/.eslintcache"],
      "dependsOn": ["^lint:fix"]
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "dependsOn": ["^format"]
    },
    "format:check": {
      "outputs": [".cache/.prettiercache"],
      "dependsOn": ["^format:check"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    }
  }
}
